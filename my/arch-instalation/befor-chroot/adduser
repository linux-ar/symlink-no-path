#!/bin/bash
clear

echo -e "\n**************************\n\n\t 	start ff 5	n\n\n**************************\n" 



use_add_user(){
	useradd -m -G wheel -s /bin/bash t
	echo "t:t" 	| chpasswd
	echo "root:t" 	| chpasswd
}


#++++++++++++++++++++++++++++++++
#		ff		+
#++++++++++++++++++++++++++++++++

	use_add_user

